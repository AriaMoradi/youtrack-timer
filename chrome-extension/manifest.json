{
  "content_scripts": [
    {
      "js": [
        "js/modules/api.js",
        "js/content-script.js"
      ],
      "matches": [
        "https://*.youtrack.cloud/*"
      ],
      "css": [
        "css/yt-timer.css"
      ]
    }
  ],
  "description": "This extension adds Time Tracker for YouTrack.",
  "icons": {
    "128": "images/h-app@128px.png",
    "16": "images/h-app@16px.png",
    "48": "images/h-app@48px.png"
  },
  "manifest_version": 3,
  "name": "YouTrack Time Tracker",
  "permissions": [
    "https://accounts.google.com/*",
    "https://*.youtrack.cloud/*",
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.5.9.1"
}
